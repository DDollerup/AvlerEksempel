@model List<Breeder>
@{
    ViewBag.Title = "Breeders";
}

<h2>Breeders</h2>
<table>
    <tr>
        <th>
            Name
        </th>
        <th>
            Action
        </th>
    </tr>
    @foreach (Breeder item in Model)
    {
        <tr>
            <td>
                @item.Name
            </td>
            <td>
                <a href="/Home/AddBreederConnection/@item.ID">Edit Connections</a>
                <a href="/Home/DeleteBreederConnection/@item.ID">Delete Connections</a>

            </td>
        </tr>
    }
</table>